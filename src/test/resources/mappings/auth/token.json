{
    "request": {
        "urlPath": "/auth/realms/lalahtalks/protocol/openid-connect/token",
        "method": "POST",
        "bodyPatterns": [
            {
                "equalTo": "grant_type=client_credentials"
            }
        ]
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "access_token": "abc",
            "token_type": "bearer",
            "expires_in": 3600,
            "example_parameter": "example_value"
        },
        "headers": {
            "Content-Type": "application/json"
        }
    }
}
